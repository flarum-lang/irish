{
	"name": "flarum-lang/irish",
	"type": "flarum-extension",
	"description": "Irish language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/irish",
	"keywords": [
		"locale",
		"flarum",
		"flarum-locale",
		"ga-locale",
		"irish-locale",
		"irish"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/irish/issues",
		"source": "https://github.com/flarum-lang/irish"
	},
	"require": {
		"flarum/core": "^1.6"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Irish",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "ga",
			"title": "Irish"
		},
		"extiverse": {
			"discuss": ""
		}
	}
}
